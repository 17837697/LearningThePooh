{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.3.11",
  "default_locale": "zh_CN",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "browser_action": {
    "default_icon": "img/16.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "js/login.js"
      ],
      "matches": [
        "*://pc.xuexi.cn/points/login.html*"
      ]
    },
    {
      "js": [
        "js/points.js"
      ],
      "matches": [
        "*://pc.xuexi.cn/points/my-points.html*"
      ]
    },
    {
      "js": [
        "js/article.js"
      ],
      "matches": [
        "*://www.xuexi.cn/*/e43e220633a65f9b6d8b53712cba9caa.html*"
      ]
    },
    {
      "js": [
        "js/video.js"
      ],
      "matches": [
        "*://www.xuexi.cn/*/cf94877c29e1c685574e0226618fb1be.html*"
      ]
    },
    {
      "js": [
        "js/redirect.js"
      ],
      "matches": [
        "*://*.xuexi.cn/*"
      ]
    }
  ],
  "permissions": [
    "*://*.xuexi.cn/*",
    "tabs",
    "storage",
    "notifications",
    "contextMenus"
  ]
}
